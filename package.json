{
    "name": "@sascha-nabrotzky/component-library",
    "version": "0.0.1",
    "description": "My personal component library",
    "type": "module",
    "main": "dist/sn-ui.es.js",
    "types": "dist/index.d.ts",
    "styles": "dist/component-library.css",
    "keywords": [
        "component",
        "library",
        "ui"
    ],
    "license": "UNLICENSED",
    "author": {
        "name": "Sascha Nabrotzky"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sascha-nabrotzky/component-library.git"
    },
    "exports": {
        ".": {
            "import": "./dist/sn-ui.es.js",
            "require": "./dist/sn-ui.umd.js"
        },
        "./component-library": {
            "require": "./component-library.css",
            "default": "./component-library.css"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint .",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^4.1.1",
        "@eslint/js": "^9.36.0",
        "@storybook/addon-a11y": "^9.1.7",
        "@storybook/addon-docs": "^9.1.7",
        "@storybook/addon-vitest": "^9.1.7",
        "@storybook/react-vite": "^9.1.7",
        "@types/node": "^24.5.2",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react-swc": "^4.1.0",
        "@vitest/browser": "^3.2.4",
        "@vitest/coverage-v8": "^3.2.4",
        "eslint": "^9.36.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "eslint-plugin-storybook": "^9.1.7",
        "globals": "^16.4.0",
        "playwright": "^1.55.0",
        "storybook": "^9.1.7",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.44.0",
        "vite": "^7.1.7",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.2.4"
    }
}
